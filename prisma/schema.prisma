generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Website{
  id String @id @default(uuid())
  url String @unique
  brandName String?
  description String?
  isEnhanced  Boolean  @default(false) 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
